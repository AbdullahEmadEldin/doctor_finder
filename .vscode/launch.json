    "version": "0.2.0",
    "configurations": [
        {
            "name": "doctor_finder",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "doctor_finder (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "doctor_finder (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Doc-Finder Development",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
            "args": ["--flavor", "development", "--target", "lib/main_development.dart" ]
        },
        {
            "name": "Doc-Finder Production",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_production.dart",
            "args": ["--flavor", "production", "--target", "lib/main_production.dart" ]
        }
    ]
}